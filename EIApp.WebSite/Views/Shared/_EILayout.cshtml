<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>>@ViewBag.Title - 我的 EIAPP 应用程序</title>
    <link href="~/Content/themes/gray/easyui.css" rel="stylesheet" />
    <link href="~/Content/themes/icon.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/Scripts/jquery.easyui.min.js"></script>
    <script src="~/Scripts/easyui-lang-zh_CN.js"></script>
    <script src="~/Scripts/jscommon.js"></script>
    @*@Styles.Render("~/Content/themes/gray/css")*@
    @*@Scripts.Render("~/myscript/js")*@
    
</head>
<body class="easyui-layout">
    <div data-options="region:'north',border:false" style="height: 100px; background: #2e3e4e;"></div>
    <div data-options="region:'south',title:'CopyRight © 2010 - 2016 By Learn',collapsible:false"></div>
    <div data-options="region:'west',title:'导航菜单',split:true" style="width:150px;">
        @*其他部份内容*@
        @RenderSection("MasterPart", false)
    </div>
    <div data-options="region:'center',border:false">

        @RenderBody()
    </div>
</body>
</html>